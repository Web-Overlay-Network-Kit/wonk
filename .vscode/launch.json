	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug executable 'relay'",
			"cargo": {
				"args": [
					"build",
					"--bin=relay",
					"--package=relay"
				],
				"filter": {
					"name": "relay",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in executable 'relay'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--bin=relay",
					"--package=relay"
				],
				"filter": {
					"name": "relay",
					"kind": "bin"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in library 'proxy-rs'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--lib",
					"--package=proxy-rs"
				],
				"filter": {
					"name": "proxy-rs",
					"kind": "lib"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		}
	]
}